{
  "name": "NBAForum",
  "displayName": "NBAForum",
  "expo": {
    "plugins": [[
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
    ]],
    "name": "NBAForum",
    "slug": "NBAForum",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.buse23.NBAForum"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "56d3e37e-8ee5-4074-8ef0-684556bbabeb"
      }
    }
  }
}
